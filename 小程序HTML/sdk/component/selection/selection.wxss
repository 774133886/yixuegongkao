/* plugin/components/selection/selection.wxss */
.options-wrapper {
    padding: 10px;
    background: #333333;
    color: white;
}
.options {
    line-height: 30px;
}
.options.active {
    color: #1795FF;
}

.value-wrapper {
    color: white;
    border: 1px solid white;
    background: black;
    opacity: 0.7;
    border-radius: 2px;
    line-height: 30px;
    width: 50px;
    text-align: center;
}
